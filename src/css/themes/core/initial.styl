.col-count-product
  padding-top 10px
  margin-top 10px
  margin-bottom 10px
  border-top 1px solid #DDDDDD
.input-count-product
  background-color transparent
  border none
  border-bottom 2px solid $secondary
  text-align center
  font-size 1.375rem
  color $secondary
  max-width 130px
  padding-top 7px
  padding-bottom 7px
  &:focus
    outline -webkit-focus-ring-color auto 0px
    
.table-responsive
  display block
  width 100%
  overflow-x auto
  -webkit-overflow-scrolling touch
  -ms-overflow-style -ms-autohiding-scrollbar
  
.table
  width 100%
  max-width  100%
  margin-bottom  1rem
  background-color  transparent
  color $secondary
  font-size 1.125rem
  th,td
    padding: 1rem
  thead th
    color $secondary
    font-size 1.375rem
    vertical-align bottom
    border-bottom 1px solid #DDDDDD
  tbody tr
    vertical-align middle
    &:not(:last-child) td
      border-bottom 1px solid #DDDDDD
  tfoot td
    border-top 1px solid #DDDDDD
    border-bottom 1px solid #DDDDDD
    text-align center
    color $secondary
    font-weight bold
    font-size 1.375rem
  &-price
    text-align: center
    span
      font-weight bold
    &__total
      color #EC3800
      margin-left 5px
  &-description
    width 180px
    &--boder
      border-right 1px solid #DDDDDD;
      border-left 1px solid #DDDDDD;
    &--info
      @media screen and (min-width: 700px)
        min-width 346px
    &__img
      height 164px
      width 164px
      border: 1px solid #DDDDDD;
      border-radius: 7px;
      margin: 0px 5px
      img
        width 100%
        max-height 100%
    p
      margin 0

p
  color: rgb(76, 61, 58);
  font-family: Muli;
  font-size: 1.25rem;

.m-w-100
  max-width: 100%;